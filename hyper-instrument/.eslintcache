  }\n\n  return (\n    <Modal\n      open={true}\n      onClose={props.onCloseListener}>\n      <Paper className={classes.paper}>\n        <Typography className={classes.title} variant='h3'>Define your scale</Typography>\n\n        <Container>\n          <Box display='flex' width='100%' justifyContent='space-evenly'>\n            {slots}\n          </Box>\n        </Container>\n\n        <Button className={classes.button} onClick={handleButtonClick} variant='contained' color='primary'>Save scale</Button>\n\n\n\n      </Paper>\n    </Modal>\n  );\n}\n\n\nconst replaceCharAtIndex = (str, idx, char) => {\n  return str.replace(/./g, (c, i) => i == idx ? char : c);\n}\n\n\n\nconst useNoteStyles = makeStyles((theme) => ({\n  root: {\n    width: theme.spacing(3),\n    height: theme.spacing(3),\n    '&$selected': {\n      background: '#1D2225'\n    }\n  },\n  selected: {\n    background: '#1D2225'\n  }\n\n}));\n\nconst NoteSlot = (props) => {\n\n  const classes = useNoteStyles();\n\n  return (\n    <ToggleButton classes={{ root: classes.root, selected: classes.selected }} onChange={props.onChange} selected={props.selected} />\n  )\n\n}","/Users/David 1/Home/NYC/Convocatorias/NIME 2021/Dev/hyper-instrument/hyper-instrument/src/components/PlaybackControls.js",["122","123","124"],"/Users/David 1/Home/NYC/Convocatorias/NIME 2021/Dev/hyper-instrument/hyper-instrument/src/store/Store.js",[],"/Users/David 1/Home/NYC/Convocatorias/NIME 2021/Dev/hyper-instrument/hyper-instrument/src/store/appSessionSlice.js",[],"/Users/David 1/Home/NYC/Convocatorias/NIME 2021/Dev/hyper-instrument/hyper-instrument/src/components/ActionBar.js",["125","126"],"/Users/David 1/Home/NYC/Convocatorias/NIME 2021/Dev/hyper-instrument/hyper-instrument/src/components/PlayButton.js",[],"/Users/David 1/Home/NYC/Convocatorias/NIME 2021/Dev/hyper-instrument/hyper-instrument/src/components/StopButton.js",["127"],"/Users/David 1/Home/NYC/Convocatorias/NIME 2021/Dev/hyper-instrument/hyper-instrument/src/components/Inp